    <script>

        function ReplaceNumberWithCommas(myNumber) {
            var n = myNumber.toString().split(".");
            n[0] = n[0].replace(/\B(?=(\d{3})+(?!\d))/g, ",");
            return n.join(".");
        }

        function ConfirmDelete(item, id) {
            event.preventDefault();
